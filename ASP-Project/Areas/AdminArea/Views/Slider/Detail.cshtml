@model Slider
@{
    ViewData["Title"] = "Index";
}
<div class="container">
    <div class="container-fluid my-3 mx-3">
        <div class="table-responsive pt-3">
            
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>
                            Image
                        </th>
                        <th>
                            SubTitle
                        </th>
                        <th>
                            Title
                        </th>
                        <th>
                            Desc
                        </th>
                        
                    </tr>
                </thead>
                <tbody>
                    
                        <tr>
                            <td>
                                <img class="w-25 h-25" class src="~/assets/img/slider/@Model.Image" alt="slider" />
                            </td>
                            <td>
                                @Model.SubTitle
                            </td>
                            <td>
                            @Model.Title
                            </td>
                            <td>
                            @Model.Desc
                            </td>
                            <td style="text-align: center;">
                            </td>
                        </tr>


                </tbody>
            </table>
        </div>
    </div>
</div>

